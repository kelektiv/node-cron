	"engines": {
		"node": ">=18.x"
	},
	"files": [
		"dist/**/*.js",
		"dist/**/*.d.ts",
		"CHANGELOG.md",
		"LICENSE",
		"README.md"
	],
	"lint-staged": {
		"*.ts": "eslint src/ tests/ --fix",
		"*.{json,md,yml}": "prettier ./**/*.{json,md,yml} --check --write"
	}
}
